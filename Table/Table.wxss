.table{
  /* border-right: 1px solid #000; */
  text-align: center;
  flex:1;
}
.table-title{
  width:100%;
  height: 40px;
  line-height: 40px;
  background:#e3e3e3;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.table-data{
  width:100%;
  height: 40px;
  line-height: 40px;
  background:#fff;
  display: flex; 
  flex-direction: row; 
  justify-content: space-between;
}
.table-column{
  text-align: center;
  flex:1;
  display: flex; 
  flex-direction: column;
  /* border-right:1px solid #000; */
}
.table-data:nth-child(odd){
  background: rgb(209, 241, 252);
}